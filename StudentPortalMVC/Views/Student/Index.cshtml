@model List<Student>

<h2>@ViewData["Title"]</h2>
<p>@ViewBag.Message</p>
<p>@TempData["Note"]</p>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Course</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Course</td>
                <td>
                    <a class="button" href="/Student/Details/@student.Id">View</a>
                </td>
            </tr>
        }
    </tbody>
</table>
